{
  "name": "vertjs",
  "version": "1.0.0",
  "author": "gouz",
  "devDependencies": {
    "@types/bun": "latest",
    "bun-lightningcss": "^0.2.0",
    "bun-plugin-html": "^2.0.0",
    "css-select": "^5.1.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^5.1.3",
    "html-minifier": "^4.0.0",
    "htmlparser2": "^9.1.0",
    "jsonminify": "^0.4.2",
    "lightningcss": "^1.19.0",
    "prettier": "^3.2.5",
    "svgo": "^3.0.2",
    "typescript-eslint": "^7.0.2"
  },
  "bin": {
    "vertjs": "./bin/index.js"
  },
  "description": "A green-it framework",
  "keywords": [
    "green-it",
    "vertjs"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "dev": "bun render && bun serve",
    "serve": "bun src/index.ts serve example/dist",
    "render": "bun src/index.ts render example",
    "make": "bun build src/index.ts --outdir bin --minify --target bun --sourcemap=none",
    "make:exe": "bun build src/index.ts --outfile exe/vertjs --compile --minify --sourcemap=none"
  },
  "dependencies": {
    "@bogeychan/elysia-etag": "^0.0.3",
    "@elysiajs/static": "^0.7.1",
    "@types/html-minifier": "^4.0.5",
    "@types/jsonminify": "^0.4.3",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "elysia": "^0.8.15",
    "elysia-autoroutes": "^0.4.0"
  }
}
